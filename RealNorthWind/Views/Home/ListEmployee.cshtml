
@{
    ViewBag.Title = "ListEmployee";
}

<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>

<h1>List Of Employees</h1><br />
<table>
    <tr>
        <th>Employee Id</th>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Title</th>
        <th>Title Of Courtesy</th>
        <th>Birthdate</th>
        <th>Hiredate</th>
        <th>Address</th>
        <th>City</th>
        <th>Region</th>
        <th>Postal Code</th>
        <th>Country</th>
        <th>Home Phone</th>
        <th>Extension</th>
        <th>Photo</th>
        <th>Notes</th>
        <th>Reports To</th>
        <th>Update</th>
    </tr>

    @foreach (var a in ViewBag.aListOfEmployee)
    {
        <tr>
            <td>@Html.Raw(a.EmployeeId)</td>
            <td>@Html.Raw(a.LastName)</td>
            <td>@Html.Raw(a.FirstName)</td>
            <td>@Html.Raw(a.Title)</td>
            <td>@Html.Raw(a.TitleOfCourtesy)</td>
            <td>@Html.Raw(a.BirthDate)</td>
            <td>@Html.Raw(a.HireDate)</td>
            <td>@Html.Raw(a.Address)</td>
            <td>@Html.Raw(a.City)</td>
            <td>@Html.Raw(a.Region)</td>
            <td>@Html.Raw(a.PostalCode)</td>
            <td>@Html.Raw(a.Country)</td>
            <td>@Html.Raw(a.HomePhone)</td>
            <td>@Html.Raw(a.Extension)</td>
            <td>@Html.Raw(a.Photo)</td>
            <td>@Html.Raw(a.Notes)</td>
            <td>@Html.Raw(a.ReportsTo)</td>
            <td><a href="GetUpdateEmployee?id=@a.EmployeeId">Update</a></td>
        </tr>
    }
</table><br /><br />